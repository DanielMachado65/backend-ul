  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:docs": "npx @compodoc/compodoc -p tsconfig.json -s",
    "start:dev": "nest start --watch",
    "start:dev2": "NODE_OPTIONS=\"--max-old-space-size=8192\" nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:repl": "npm run start -- --entryFile repl",
    "lint": "eslint --config ./.eslintrc.js \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config .jestrc.json",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config .jestrc.json",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "healthcheck": "node health-checker.js",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org checklist --project uluru-api ./dist && sentry-cli sourcemaps upload --org checklist --project uluru-api ./dist"
  },
